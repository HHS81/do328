<?xml version="1.0"?>

<PropertyList>
    <name>options</name>
    <layout>vbox</layout>

    <text>
        <label>Options</label>
        <halign>center</halign>
    </text>



  <button>
    <legend>Close</legend>
    <default>true</default>
    <key>Esc</key>
    <binding>
      <command>dialog-close</command>
    </binding>
  </button>

</PropertyList>
