         <file>Aircraft/do328/Nasal/lightmap.nas</file>
         <file>Aircraft/do328/Nasal/lightmap.nas</file>
         <file>Aircraft/do328/Nasal/Autostart.nas</file>
         <file>Aircraft/do328/Nasal/pressurization.nas</file>
      </systems>
      <efis>
