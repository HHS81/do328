<PropertyList include="do328-common.xml">
   <sim>
      <description>Dornier 328-300</description>
      <author>HHS (3d, config, fdm); xcvb85 (EICAS, RMU, systems); clm76 (CDU, RCU); Jentron (prop and turbine files)</author>
      <status>devel</status>
      <flight-model>jsb</flight-model>
      <aero>do328-300</aero>
      <fuel-fraction>0.5</fuel-fraction>
      <tags>
         <tag>jet</tag>
         <tag>dornier</tag>
         <tag>retractable-gear</tag>
         <tag>ifr</tag>
         <tag>twin-engine</tag>
         <tag>glass-cockpit</tag>
      </tags>
      <!--to select the center pedestal type-->
<aircraft-type>
<prop>0</prop>
</aircraft-type>

      <startup>
         <splash-texture>Aircraft/do328/splashjet.png</splash-texture>
      </startup>
      <model>
         <path>Aircraft/do328/Models/do328jet.xml</path>
         <variant type="int">0</variant>
         <livery>
            <file type="string">do328jet_liveries/Default.png</file>
         </livery>
         <do328>
            <material>
               <fuselage>
                  <diffuse>
                     <red>1.0</red>
                     <green>1.0</green>
                     <blue>1.0</blue>
                  </diffuse>
                  <specular>
                     <red>1.0</red>
                     <green>1.0</green>
                     <blue>1.0</blue>
                  </specular>
                  <ambient>
                     <red>1.0</red>
                     <green>1.0</green>
                     <blue>1.0</blue>
                  </ambient>
               </fuselage>
            </material>
         </do328>
      </model>
      <rendering>
         <refl_correction type="float">-0.15</refl_correction>
         <fresnel-factor type="float">0.33</fresnel-factor>
         <refl-type type="int">1</refl-type>
      </rendering>
      <sound>
         <path>Aircraft/do328/Sounds/do328jet-sound.xml</path>
      </sound>
      <menubar>
         <default>
            <menu n="100">
               <label>328-300</label>
               <enabled type="bool">true</enabled>
               <item>
                  <label>Autostart</label>
                  <binding>
                     <command>property-toggle</command>
                     <property>sim/model/autostart</property>
                  </binding>
               </item>
               <item>
                  <label>Select Livery</label>
                  <binding>
                     <command>nasal</command>
                     <script>aircraft.livery.dialog.toggle()</script>
                  </binding>
               </item>
               <item>
                  <label>Tiller Steering</label>
                  <binding>
                     <command>nasal</command>
                     <script>dialogs.tiller_steering.open()</script>
                  </binding>
               </item>
            </menu>
         </default>
      </menubar>
   </sim>
   <nasal>
      <systems>
         <file>Aircraft/do328/Nasal/common_systems.nas</file>
         <file>Aircraft/do328/Nasal/jet_systems.nas</file>
         <file>Aircraft/do328/Nasal/flightdirector.nas</file>
         <file>Aircraft/do328/Nasal/Electrical.nas</file>
         <file>Aircraft/do328/Nasal/ground_steering.nas</file>
