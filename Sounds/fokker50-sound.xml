<?xml version="1.0"?>
 
<PropertyList>

 <fx>

  <engine0>
   <name>whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <position>
    <x>4.7</x>
    <y>2.9</y>
    <z>0.4</z>
   </position>
   <property>/engines/engine[0]/running</property>
   <volume>
     <factor>0.00025</factor>
   </volume> 
   <pitch>
    <property>/engines/engine[0]/n1</property>
    <type>lin</type>
    <factor>.398</factor>
   </pitch>
  </engine0>

  <engine0>
   <name>engine0</name>
   <mode>looped</mode>
   <path>Sounds/turboprop1.wav</path>
   <position>
    <x>4.7</x>
    <y>2.9</y>
    <z>0.4</z>
   </position>
   <condition>
    <property>/engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>abs</type>
    <factor>0.00005</factor>
    <offset>-0</offset>
    <min>0.15</min>
    <max>0.35</max>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <factor>0.00998</factor>
    <offset>0.9</offset>
    <min>0.7</min>
    <max>10</max>
   </pitch>
  </engine0>

  <engine1>
   <name>whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <position>
    <x>4.7</x>
    <y>-2.9</y>
    <z>0.4</z>
   </position>
   <property>/engines/engine[1]/running</property>
   <volume>
     <factor>0.00025</factor>
   </volume>
   <pitch>
    <property>/engines/engine[1]/n1</property>
    <type>lin</type>
    <factor>.417</factor>
   </pitch>
  </engine1>

  <engine1>
   <name>engine1</name>
   <mode>looped</mode>
   <path>Sounds/turboprop1.wav</path>
   <position>
    <x>4.7</x>
    <y>-2.9</y>
    <z>0.4</z>
   </position>
   <condition>
    <property>/engines/engine[1]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[1]/thruster/rpm</property>
    <type>abs</type>
    <factor>0.00069</factor>
    <offset>-0.328</offset>
    <min>0.0</min>
    <max>1.0</max>
   </volume> 
   <pitch>
    <property>/engines/engine[1]/thruster/rpm</property>
    <factor>0.00113</factor>
    <offset>0.9</offset>
    <min>0.65</min>
    <max>10</max>
   </pitch>
  </engine1>

  <tire-rumble>
   <name>tire-rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/groundspeed-kt</property>
    <factor>0.01</factor>
    <max>0.9</max>
   </volume>
  </tire-rumble>

  <gear-wind>
   <name>gear-wind</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
     <greater-than>
      <property>/gear/gear[1]/position-norm[0]</property>
      <value>0.01</value>
     </greater-than>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.004</factor>
    <max>1.0</max>
   </volume>
  </gear-wind>

  <gear-lock>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear-lock>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind.wav</path>
   <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
   <volume>
     <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
     <factor>0.00075</factor>
     <min>0.03</min>
     <max>0.25</max>
   </volume>
   <pitch>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>

  <buffet>
   <name>alpha-buffet</name>
   <mode>looped</mode>
   <condition>
    <greater-than>
     <property>/fdm/jsbsim/aero/alpha-rad[0]</property>
     <value>0.2</value>
    </greater-than>
   </condition>
   <path>Sounds/rumble.wav</path>
   <volume>
    <property>/fdm/jsbsim/aero/alpha-rad[0]</property>
    <factor>15.0</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/fdm/jsbsim/aero/alpha-rad[0]</property>
    <factor>0.1</factor>
   </pitch>
  </buffet>

<!-- New overspeed sound! -->
  <clacker>
   <name>clacker</name>
   <mode>looped</mode>
   <type>avionics</type>
   <path>Sounds/overspeed.wav</path>
   <condition>
    <greater-than>
     <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
     <value>335</value>
    </greater-than>
   </condition>
  </clacker>

<!-- generic GPWS, borrowed from B1900D-->

