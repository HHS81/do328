<?xml version="1.0"?>
<PropertyList>
<fx>
    <beep>
        <name>overspeed</name>
        <path>Sounds/beep.wav</path>
        <property>instrumentation/mk-viii/inputs/discretes/beep</property>
        <mode>looped</mode>
    </beep>
    <beep>
        <name>tcas</name>
        <path>tcas.wav</path>
        <property>instrumentation/efis/tcas</property>
        <mode>once</mode>
    </beep>
    <beep>
        <name>squelch</name>
        <path>radio-5s.wav</path>
        <property>instrumentation/rcu/squelch</property>
        <mode>looped</mode>
    </beep>

<!-- Engine 1 -->
    <engine>
        <name>engine left outside</name>
        <path>fullpower.wav</path>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>1</value>
                </greater-than>
                <not>
                    <property>sim/current-view/internal</property>
                </not>
            </and>
        </condition>
        <mode>looped</mode>
        <volume>
            <property>engines/engine[0]/thruster/rpm</property>
            <factor>0.00077</factor>
        </volume>
        <pitch>
                <property>engines/engine[0]/thruster/rpm</property>
                <offset>0.0</offset>
                <factor>0.0007692307692308</factor>
                <min>0.5</min>
                <max>1.5</max>
        </pitch>
        <position>
            <x>-3.5</x>
            <y>-3.5</y>
            <z>0.3</z>
        </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>180</inner-angle>
                <outer-angle>360</outer-angle>
                <outer-gain>0.5</outer-gain>
                <reference-dist>20</reference-dist>
            </orientation>
    </engine>

    <engine>
        <name>engine left inside</name>
        <path>fullpower-int.wav</path>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>1</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <mode>looped</mode>
        <volume>
            <property>engines/engine[0]/thruster/rpm</property>
            <factor>0.00077</factor>
        </volume>
        <pitch>
                <property>engines/engine[0]/thruster/rpm</property>
                <offset>0.0</offset>
                <factor>0.0007692307692308</factor>
                <min>0.5</min>
                <max>1.5</max>
        </pitch>
        <position>
            <x>-3.5</x>
            <y>-3.5</y>
            <z>0.3</z>
        </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>180</inner-angle>
                <outer-angle>360</outer-angle>
                <outer-gain>0.5</outer-gain>
                <reference-dist>20</reference-dist>
            </orientation>
    </engine>

<!-- Engine 2 -->
    <engine>
        <name>engine right outside</name>
        <path>fullpower.wav</path>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>1</value>
                </greater-than>
                <not>
                    <property>sim/current-view/internal</property>
                </not>
            </and>
        </condition>
        <mode>looped</mode>
        <volume>
            <property>engines/engine[0]/thruster/rpm</property>
            <factor>0.00077</factor>
        </volume>
        <pitch>
                <property>engines/engine[0]/thruster/rpm</property>
                <offset>0.0</offset>
                <factor>0.0007692307692308</factor>
                <min>0.5</min>
                <max>1.5</max>
        </pitch>
        <position>
            <x>-3.5</x>
            <y>3.5</y>
            <z>0.3</z>
        </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>180</inner-angle>
                <outer-angle>360</outer-angle>
                <outer-gain>0.5</outer-gain>
                <reference-dist>20</reference-dist>
            </orientation>
    </engine>

    <engine>
        <name>engine right inside</name>
        <path>fullpower-int.wav</path>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>1</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <mode>looped</mode>
        <volume>
            <property>engines/engine[0]/thruster/rpm</property>
            <factor>0.00077</factor>
        </volume>
        <pitch>
                <property>engines/engine[0]/thruster/rpm</property>
                <offset>0.0</offset>
                <factor>0.0007692307692308</factor>
                <min>0.5</min>
                <max>1.5</max>
        </pitch>
        <position>
            <x>-3.5</x>
            <y>3.5</y>
            <z>0.3</z>
        </position>
            <orientation>
                <x>1</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>180</inner-angle>
                <outer-angle>360</outer-angle>
                <outer-gain>0.5</outer-gain>
                <reference-dist>20</reference-dist>
            </orientation>
    </engine>

<!-- SFX -->
    <rumblefront>
        <name>nose wheel rumble</name>
        <mode>looped</mode>
        <path>Sounds/rattleroll.wav</path>
        <condition>
            <or>
                <property>gear/gear[0]/wow</property>
                <property>gear/gear[1]/wow</property>
                <property>gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>/velocities/groundspeed-kt</property>
            <factor>0.1</factor>
            <max>3.0</max>
        </volume>
        <position>
            <x>-3</x>
            <y>0</y>
            <z>-1</z>
        </position>
    </rumblefront>

    <gear>
        <name>gear-lock</name>
        <path>Sounds/gear-lck.wav</path>
        <condition>
            <or>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1</value>
                </equals>
            </or>
        </condition>
        <mode>once</mode>
        <position>
            <x>-9</x>
            <y>0</y>
            <z>0</z>
        </position>
    </gear>

    <wind>
        <name>wind</name>
        <path>Sounds/wind.wav</path>
        <property>velocities/airspeed-kt</property>
        <mode>looped</mode>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.002</factor>
            <min>0.0</min>
            <max>0.15</max>
        </volume>
        <pitch>
            <property>velocities/airspeed-kt</property>
            <factor>0.01</factor>
            <offset>1.25</offset>
        </pitch>
        <position>
            <x>2</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>1</reference-dist>
        <max-dist>5.0</max-dist>
    </wind>
</fx>
</PropertyList>
