        <engine>
   <name>leftengine</name>
   <path>jetengine.wav</path>
   <mode>looped</mode>
   <condition>
<greater-than>
<property>/sim/current-view/view-number</property>
<value>0</value>
</greater-than>
</condition>
      <volume>
    <property>/engines/engine[0]/n1</property>
        <factor>0.01</factor>
	<max>2.0</max>
    <offset>0.00</offset>
   </volume>
   <pitch>
<property>/engines/engine[0]/n1</property>
<offset>0.25</offset>
<factor>0.01</factor>
<max>1</max>
</pitch>
            <position>
                 <x>-1.5</x>
                <y>2.5</y>
                <z>-0.33</z>
            </position>
     <orientation>
        <x> 1.0 </x>
        <y> 0.0 </y>
        <z> 0.0 </z>
        <inner-angle>90</inner-angle>
        <outer-angle>180</outer-angle>
        <outer-gain>0.1</outer-gain>
      </orientation>
        </engine>

 

