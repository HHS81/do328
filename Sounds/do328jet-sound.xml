<owind>
<name>Outside_wind</name>
<mode>looped</mode>
<path>wind-8khz.wav</path>
<condition>
<greater-than>
<property>/sim/current-view/view-number</property>
<value>0</value>
</greater-than>
</condition>
 <property>/velocities/airspeed-kt</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
<volume>
    <property>/velocities/airspeed-kt</property>
        <factor>0.011</factor>
	<max>2.0</max>
    <offset>0.00</offset>
   </volume> <position>
                 <x>0.0</x>
                <y>0.0</y>
                <z>0.0</z>
            </position>
     <orientation>
        <x> 1.0 </x>
        <y> 0.0 </y>
        <z> 0.0 </z>
        <inner-angle>90</inner-angle>
        <outer-angle>180</outer-angle>
        <outer-gain>0.1</outer-gain>
      </orientation>
</owind>

