<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>ALT.ac</path>
	<animation>
		<type>material</type>
		<object-name>iastape</object-name>
		<object-name>ALT.face</object-name>
		<object-name>ALT.needle</object-name>
		<object-name>alttxt.001</object-name>
		<object-name>alttxt.002</object-name>
		<object-name>alttxt.003</object-name>
		<object-name>alttxt.004</object-name>
		<object-name>alttxt</object-name>
		<object-name>inhg.001</object-name>
		<object-name>inhg.002</object-name>
		<object-name>inhg.003</object-name>
		<object-name>inhg.004</object-name>
		<object-name>kpa.001</object-name>
		<object-name>kpa.002</object-name>
		<object-name>kpa.003</object-name>
		<object-name>kpa.004</object-name>
		<condition>
			<or>
				<property>systems/electrical/left-emer-bus</property>
				<property>systems/electrical/left-bus-norm</property>
			</or>
		</condition>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>iastape</object-name>
		<property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0.0</dep>
			</entry>
			<entry>
				<ind>60.0</ind>
				<dep>0.0244</dep>
			</entry>
			<entry>
				<ind>300.0</ind>
				<dep>0.4120</dep>
			</entry>
			<entry>
				<ind>580</ind>
				<dep>0.8697</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>ALT.needle</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.036</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0.014</z-m>
		</center>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alttxt.004</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.1</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alttxt.003</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alttxt.002</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alttxt.001</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alttxt</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.00001</factor>
		<step>10000</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.001</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.002</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.003</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>1.0</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.004</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>10.0</factor>
		<step>0.01</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>kpa.001</object-name>
		<property>instrumentation/altimeter/setting-kpa</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>kpa.002</object-name>
		<property>instrumentation/altimeter/setting-kpa</property>
		<scale>3.386389</scale>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>kpa.003</object-name>
		<property>instrumentation/altimeter/setting-kpa</property>
		<scale>3.386389</scale>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>kpa.004</object-name>
		<property>instrumentation/altimeter/setting-kpa</property>
		<scale>3.386389</scale>
		<factor>1.0</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<!-- INHG KNOB -->
	<animation>
		<type>rotate</type>
		<object-name>inhg.knob</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<center>
			<x-m>0.00925</x-m>
			<y-m>-0.03106</y-m>
			<z-m>-0.03340</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>inhg.knob</object-name>
		<action>
			<name>decrease</name>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/altimeter/setting-inhg</property>
				<step>-0.01</step>
				<min>26</min>
				<max>33</max>
				<wrap>0</wrap>
			</binding>
		</action>
		<action>
			<name>increase</name>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/altimeter/setting-inhg</property>
				<step>0.01</step>
				<min>26</min>
				<max>33</max>
				<wrap>0</wrap>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>alt-setting</tooltip-id>
				<label>QNH: %.0f</label>
				<property>instrumentation/altimeter/setting-hpa</property>
			</binding>
		</hovered>
	</animation>
</PropertyList>
