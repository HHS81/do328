        <object-name>RMU.btn6</object-name>
        <object-name>RMU.nav</object-name>
        <object-name>RMU.adf</object-name>
        <condition>
						<or>
	           	<property>systems/electrical/left-emer-bus</property>
	           	<property>systems/electrical/left-bus-norm</property>
						</or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>controls/lighting/rmu[1]</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>select</type>
        <condition>
            <greater-than>
               <property>systems/electrical/outputs/comm</property>
               <value>5</value>
              </greater-than>
        </condition>
        <object-name>RMU.lamp</object-name>
    </animation>

    <animation>
        <type>material</type>
        <object-name>RMU.lamp</object-name>
        <condition>
						<or>
	           	<property>systems/electrical/left-emer-bus</property>
	           	<property>systems/electrical/left-bus-norm</property>
						</or>
        </condition>
        <emission>
            <red>0</red>
            <green>1</green>
            <blue>0</blue>
        </emission>
    </animation>

