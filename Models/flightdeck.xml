<!--MSP Mode Select Panel of Autoflightsystem-->

   <animation>
        <type>select</type>
        <object-name>FD.AP-annun</object-name>
        <condition>
            <not-equals>
                <property>autopilot/locks/AP-status</property>
                <value></value>
            </not-equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FD.stby-annun</object-name>
        <condition>
            <and>
                <equals>
                    <property>autopilot/locks/heading</property>
                    <value>ROLL</value>
                </equals>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>PTCH</value>
                </equals>
            </and>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.YD-annun</object-name>
        <condition>
            <property>autopilot/locks/yaw-damper</property>
        </condition>
    </animation>
    
   <animation>
        <type>select</type>
        <object-name>FD.hdg-annun</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/heading</property>
                <value>HDG</value>
            </equals>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.nav-annun</object-name>
        <condition>
            <or>
                <equals>
                    <property>autopilot/locks/heading</property>
                    <value>LNAV</value>
                </equals>
                <equals>
                    <property>autopilot/locks/heading</property>
                    <value>VOR</value>
                </equals>
                <equals>
                    <property>autopilot/locks/heading</property>
                    <value>LOC</value>
                </equals>
                <equals>
                    <property>autopilot/locks/heading-arm</property>
                    <value>VOR</value>
                </equals>
                <equals>
                    <property>autopilot/locks/heading-arm</property>
                    <value>LOC</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.bc-annun</object-name>
        <condition>
            <property>instrumentation/nav/back-course-btn</property>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.vnav-annun</object-name>
        <condition>
            <or>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>VALT</value>
                </equals>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>VFLC</value>
                </equals>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>VPTH</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.app-annun</object-name>
        <condition>
            <or>
                <equals>
                    <property>autopilot/locks/altitude-arm</property>
                    <value>GS</value>
                </equals>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>GS</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.alt-annun</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/altitude</property>
                <value>ALT</value>
            </equals>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.flc-annun</object-name>
        <condition>
            <or>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>FLC</value>
                </equals>
                <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>VFLC</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.vs-annun</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/altitude</property>
                <value>VS</value>
            </equals>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>FD.bank-annun</object-name>
        <condition>
            <less-than>
                <property>autopilot/settings/bank-limit</property>
                <value>27</value>
            </less-than>
        </condition>
    </animation>
    
